<%= simple_form_for(@seguimiento_historial) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :id_origen %>
    <%= f.input :codigo %>
    <%= f.input :naviera %>
    <%= f.input :nave %>
    <%= f.input :pol %>
    <%= f.input :pod %>
    <%= f.input :etd %>
    <%= f.input :eta %>
    <%= f.input :countchange %>
    <%= f.input :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
